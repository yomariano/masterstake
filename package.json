  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yomariano/masterstake/issues"
  },
  "homepage": "https://github.com/yomariano/masterstake#readme",
  "dependencies": {
    "fastify": "^1.1.1",
    "fastify-accepts": "^0.5.0",
